#include<iostream>
using namespace std;
int main ()
{
    int arr[5],i, max, min;
    
    cout << "Enter the elements of the array : \n";
    for (i = 0; i < 5; i++)
        cin >> arr[i];
     max = arr[0];    
    for (i = 0; i < 5; i++)
    {
        if (max < arr[i])
            max = arr[i];
    }
    min = arr[0];
    for (i = 0; i < 5; i++)
    {
        if (min > arr[i])
            min = arr[i];
    }
    cout << "Largest element : " << max;
    cout << "\nSmallest element : " << min;
    return 0;
}
